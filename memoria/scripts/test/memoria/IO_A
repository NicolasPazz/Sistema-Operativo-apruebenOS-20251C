WRITE EBX 20
RESIZE 128
IO TECLADO EAX EBX
WRITE AX 87
WRITE BX 65
WRITE CX 82
WRITE DX 44
WRITE ECX 11
WRITE EDX 1
MOV_OUT ECX AX
SUM ECX EDX
MOV_OUT ECX BX
SUM ECX EDX
MOV_OUT ECX CX
SUM ECX EDX
MOV_OUT ECX DX
SUM ECX EDX
